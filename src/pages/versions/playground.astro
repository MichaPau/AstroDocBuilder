---

import PageLayout from '../../layouts/PageLayout_A4.astro';

//import your styles and data
import '../../styles/minimal-style-scss.scss';
import data from '../../../public/data/private/en_std_data.json';

//if greater then 0 - splits the content to a second page
const splitSectionIndex = 4;

const pageTitle = data.title;

//add class id for each page container to scope styles

const styleScopeId = "minimal_style";


---
<PageLayout title={pageTitle} styleScope={styleScopeId} splitSectionIndex={splitSectionIndex} jsonData={JSON.stringify(data)}>
    
</PageLayout>